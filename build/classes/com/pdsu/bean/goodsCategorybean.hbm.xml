<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.pdsu.bean">
  <class name="GoodsCategoryBean" table="goodsCategory">
     <!-- id元素用于指定主键属性 -->

     <id name="categoryId" column="categoryId" type="java.lang.String">
            <generator class="assigned"></generator>
     </id>
     
     <property name="categoryName"  column="categoryName" type="java.lang.String">
          </property>
     
     <property name="ofRackid" column="ofRackid" type="java.lang.String" />    
    <!--  <many-to-one name="cangbean" column="C_id" class="CangBean" cascade="all"></many-to-one>
                                       -->
  </class>  
</hibernate-mapping>